{
	"$schema": "node_modules/wrangler/config-schema.json",
	/* "name": "tma-template-server", */
	"main": "src/index.ts",
	"compatibility_date": "2025-05-05",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"env": {
		"prod": {
			"vars": {
				"ENVIRONMENT": "prod"
			}
		},
		"dev": {
			"vars": {
				"ENVIRONMENT": "dev"
			}
		}
	}
}
